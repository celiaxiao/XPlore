<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.navucsd">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<!-- Main page; main entry of the app -->
		<activity android:name=".mainPageActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<!-- Other pages -->
		<activity android:name=".landmarkDetailsActivity"/>
		<activity android:name=".locationSelectionActivity" />
		<activity android:name=".timeSelectionActivity" />
		<activity android:name=".studentSelectionActivity" />
	</application>

</manifest>