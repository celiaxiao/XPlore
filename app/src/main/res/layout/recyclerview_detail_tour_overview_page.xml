<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floating_action_delete_button_tour_overview_page"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#182B49"
        app:borderWidth="0dp"
        app:fabCustomSize="33dp"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:srcCompat="@drawable/ic_remove_white_8dp"
        tools:layout_editor_absoluteY="108dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="39dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="89dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardview_tour_overview_page"
        android:layout_width="240dp"
        android:layout_height="84dp"
        app:layout_constraintStart_toStartOf="@+id/guideline12"
        tools:layout_editor_absoluteY="83dp" >

        <TextView
            android:id="@+id/textView_in_cardview_tour_overview_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>